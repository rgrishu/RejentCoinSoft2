    <style>
        .switch2 {
            position: relative;
            display: inline-block;
            width: 60px;
            height: 34px;
        }

            .switch2 input {
                opacity: 0;
                width: 0;
                height: 0;
            }

        .slider {
            position: absolute;
            cursor: pointer;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background-color: red;
            -webkit-transition: .4s;
            transition: .4s;
        }

            .slider:before {
                position: absolute;
                content: "";
                height: 26px;
                width: 26px;
                left: 4px;
                bottom: 4px;
                background-color: white;
                -webkit-transition: .4s;
                transition: .4s;
            }

        input:checked + .slider {
            background-color: #1BC703;
        }

        input:focus + .slider {
            box-shadow: 0 0 1px #1BC703;
        }

        input:checked + .slider:before {
            -webkit-transform: translateX(26px);
            -ms-transform: translateX(26px);
            transform: translateX(26px);
        }

        /* Rounded sliders */
        .slider.round {
            border-radius: 34px;
        }

            .slider.round:before {
                border-radius: 50%;
            }

        .mt-4-5 {
            margin-top: 1.8rem;
        }

        .text-left {
            text-align: left;
        }

        .icon-medall {
            font-size: 20px;
            float: right;
            padding-right: 5px;
        }

        .renew-btn {
            text-align: center;
            text-transform: uppercase;
            cursor: pointer;
            font-size: 16px;
            font-weight: 600;
            letter-spacing: 0px;
            position: relative;
            background-color: #e10f0f;
            border: none;
            color: #fff;
            padding: 10px;
            width: 160px;
            text-align: center;
            transition-duration: 0.4s;
            overflow: hidden;
            box-shadow: 0 4px 10px #193047;
            border-radius: 4px;
        }

            .renew-btn:hover {
                color: #fff;
            }

            .renew-btn:after {
                content: "";
                background: #e10f0f;
                display: block;
                position: absolute;
                padding-top: 300%;
                padding-left: 350%;
                margin-left: -20px !important;
                margin-top: -120%;
                opacity: 0;
                transition: all 0.8s
            }

            .renew-btn:active:after {
                padding: 0;
                margin: 0;
                opacity: 1;
                transition: 0s
            }

            .renew-btn:focus {
                outline: 0;
            }

        @keyframes glowing {
            0% {
                background-color: #e10f0f;
                box-shadow: 0 0 5px #e10f0f;
            }

            50% {
                background-color: #e10f0f;
                box-shadow: 0 0 10px #e10f0f;
            }

            100% {
                background-color: #870303;
                box-shadow: 0 0 5px #870303;
            }
        }

        .renew-btn {
            animation: glowing 1300ms infinite;
        }
    </style>
